ter.push({\n          name: \"admin\"\n        });\n        localStorage.removeItem(\"idMaitre\");\n      }).catch(function () {\n        alert(\"Impossible d'effectuer l'action sur le statut 'banni'.\");\n      });\n    },\n    makeUnMakeAdminF: function makeUnMakeAdminF() {\n      var _this = this;\n\n      this.axios.put(this.url2, this.maitre).then(function (res) {\n        // console.log(res.data);\n        _this.maitre = res.data;\n        alert(\"Statut 'admin' modifié avec succès.\");\n        Router.push({\n          name: \"admin\"\n        });\n        localStorage.removeItem(\"idMaitre\");\n      }).catch(function () {\n        alert(\"Impossible d'effectuer l'action sur le statut 'admin'.\");\n      });\n    }\n  },\n  beforeRouteEnter: function beforeRouteEnter(from, to, next) {\n    if (localStorage.getItem(\"token\") == null) {\n      next(\"/login\"); // } else if (localStorage.getItem(token.banni = 1)) {\n      //   next()\n    } else {\n      next();\n    }\n  }\n};",{"version":3,"sources":["showMaitres.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgRA,OAAA,YAAA,MAAA,gBAAA;AACA,OAAA,QAAA;AACA,OAAA,MAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,aADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,QAAA,EAAA;AADA,GAFA;AAKA,EAAA,IALA,kBAKA;AACA,WAAA;AACA,MAAA,MAAA,EAAA,EADA;AAEA,MAAA,IAAA,EAAA,EAFA;AAGA,MAAA,KAAA,EAAA,EAHA;AAIA,MAAA,IAAA,EAAA,IAJA;AAKA,MAAA,KAAA,EAAA,IALA;AAMA,MAAA,GAAA,EAAA,IANA;AAOA,MAAA,KAAA,EAAA,IAPA;AAQA,MAAA,SAAA,EAAA,IARA;AASA,MAAA,WAAA,EAAA,IATA;AAUA,MAAA,GAAA,EAAA,2CAVA;AAWA,MAAA,IAAA,EAAA;AAXA,KAAA;AAaA,GAnBA;AAoBA;AACA;AACA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA,SAAA,IAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AACA,IAAA,YAAA,CAAA,OAAA,CAAA,UAAA,EAAA,KAAA,MAAA,