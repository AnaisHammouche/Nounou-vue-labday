   // console.log(res);\n        alert(\"Votre chat a été supprimé.\");\n      }).catch(function (err) {\n        alert(err);\n      });\n    },\n    updateProfileMaitre: function updateProfileMaitre() {\n      var _this3 = this;\n\n      this.axios.post(\"http://localhost:6001/maitre/updateByEmail\", this.maitre);\n      this.chat.id_maitre = this.maitre.idMaitre; // console.log(this.chat)\n\n      this.axios.post(\"http://localhost:6001/chat/newCat\", this.chat).then(function () {\n        // console.log(res);\n        _this3.$router.push(\"/\");\n\n        alert(\"Votre profil a été mis à jour avec succès.\");\n      }).catch(function () {// console.log(err);\n      });\n    },\n    recupIdChat: function recupIdChat(idToken) {\n      var _this4 = this;\n\n      // console.log(this.url);\n      // console.log(this.url + idToken);\n      this.axios.get(this.url + idToken).then(function (res) {\n        // console.log(res.data)\n        _this4.resultatchats = res.data; // console.log(this.resultatchats);\n        // .map (boucler et retourne le resultat en tableau)\n\n        _this4.idChats = _this4.resultatchats.map(function (chat) {\n          return chat.idChat;\n        });\n        _this4.garde.id_chat = _this4.idChats; // console.log(`idChats = ${this.idChats}`);\n      }).catch(function () {// console.log(err)\n      });\n    },\n    mesGardes: function mesGardes() {\n      var _this5 = this;\n\n      this.axios.post(\"http://localhost:6001/garde/AllgardeChatsOfOneMaitre\", {\n        idChats: this.idChats\n      }).then(function (res) {\n        //  console.log(res.data);\n        _this5.resultatgardes = res.data.gardes;\n        _this5.show = true;\n      }).catch(function (err) {\n        alert(err);\n      });\n    },\n    uploadImage: function uploadImage(e) {\n      var _this6 = this;\n\n      // on affecte à la variable image, le fichier en position\n      var image = e.target.files[0]; // on créé une nouvelle instance de FileReader (qui est un constructeur) pour lire le fichier et le convertir\n\n      var reader = new FileReader(); // on convertit l'image en url via la méthode readAsDataURL\n\n      reader.readAsDataURL(image); // au chargement de la variable reader, on lui affecte l'evenement de l'input\n\n      reader.onload = function (e) {\n        // on affecte à la variable photo le résultat de la cible de l'evenement, donc le fichier uploadé\n        _this6.photo = e.target.result;\n        _this6.chat.photo = _this6.photo;\n      };\n    }\n  }\n};",{"version":3,"sources":["profilMaitre.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkRA,OAAA,YAAA,MAAA,gBAAA;AACA,OAAA,QAAA;AACA,OAAA,aAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,cADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,QAAA,EAAA,QADA;AAEA,IAAA,aAAA,EAAA;AAFA,GAFA;AAMA,EAAA,IANA,kBAMA;AACA,WAAA;AACA,MAAA,MAAA,EAAA,EADA;AAEA,MAAA,KAAA,EAAA,EAFA;AAGA,MAAA,IAAA,EAAA,EAHA;AAIA,MAAA,KAAA,EAAA,EAJA;AAKA,MAAA,KAAA,EAAA,IALA;AAMA,MAAA,IAAA,EAAA,KANA;AAOA,MAAA,aAAA,EAAA,EAPA;AAQA,MAAA,OAAA,EAAA,EARA;AASA,MAAA,cAAA,EAAA,EATA;AAUA,MAAA,GAAA,EAAA;AAVA,KAAA;AAYA,GAnBA;AAoBA,EAAA,OAAA,EAAA,mBAAA;AACA,QAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,KAFA,MAEA;AACA,WAAA,MAAA,GAAA,YAAA,CAAA,MAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,CAAA;AACA;;AAEA,SAAA,WAAA;AACA,QAAA,KAAA,GAAA,YAAA,CAAA,MAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,CAAA;AACA,QAAA,OAAA,GAAA,KAAA,CAAA,QAAA,CATA,CAUA;;AACA,SAAA,WAAA,CAAA,OAAA;AACA,GAhCA;AAkCA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AAAA;;AACA,WAAA,KAAA,CACA,GADA,uDAEA,KAAA,MAAA,CAAA,QAFA,GAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAPA,EAQA,KARA,CAQA,YAAA,CACA;AACA,OAVA;AAWA,KAbA;AAcA,IAAA,SAdA,qBAcA,KAdA,EAcA;AAAA;;AACA;AACA,WAAA,IAAA,GAAA,KA